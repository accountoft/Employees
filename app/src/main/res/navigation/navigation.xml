<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.ekipa2.o7task.ui.main.MainFragment"
        android:label="MainFragment">
        <action
            android:id="@+id/action_mainFragment_to_analyticsFragment"
            app:destination="@id/analyticsFragment" />
        <action
            android:id="@+id/action_mainFragment_to_addEmplFragment"
            app:destination="@id/addEmplFragment" />
    </fragment>
    <fragment
        android:id="@+id/analyticsFragment"
        android:name="com.ekipa2.o7task.ui.main.AnalyticsFragment"
        android:label="analytics_fragment"
        tools:layout="@layout/analytics_fragment" />
    <fragment
        android:id="@+id/addEmplFragment"
        android:name="com.ekipa2.o7task.AddEmplFragment"
        android:label="AddEmplFragment" />
</navigation>